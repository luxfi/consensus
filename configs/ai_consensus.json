  "mlx_config": {
    "model_path": "/models/consensus/mlx_model.bin",
    "device_type": "metal",
    "batch_size": 32,
    "enable_quantization": true
  },
  "performance": {
    "cache_size": 10000,
    "batch_processing": true,
    "parallel_ops": 16,
    "max_latency": 50
  },
  "debug": false
}